{
  "name": "lintme",
  "version": "1.0.0",
  "author": "Hima Mynampaty",
  "description": "Composable markdown lint rule builder and engine",
  "private": true,
"scripts": {
  "test": "vitest run",
  "dev": "concurrently -n backend -c blue \"yarn workspace backend start\"",
  "build:functions": "esbuild netlify/functions-src/runPipeline.js --bundle --platform=node --outdir=netlify/functions --packages=external"

},
  "workspaces": [
    "apps/*",
    "packages/*",
    "shared/*"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.6",
    "postcss": "^8.5.4",
    "tailwindcss": "^3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "firebase": "^11.10.0",
    "idb": "^8.0.3",
    "mdast-util-gfm": "^3.1.0",
    "micromark-extension-gfm": "^3.0.0"
    
  }
}
